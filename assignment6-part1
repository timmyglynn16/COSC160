  Program6contd pc = new Program6contd();
  
  // call method for #1
  pc.num1();
  
  // call method for #2
  pc.num2();
  
  // call method for #3
  pc.num3();
  
  // call method for #4
  pc.num4();
  
  // call method for #5
  pc.num5();
  
  // call method for #6
  pc.num6();
  
  // call method for #7
  pc.num7();
 }
}
